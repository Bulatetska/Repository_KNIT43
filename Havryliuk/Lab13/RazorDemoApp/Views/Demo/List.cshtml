@model List<string>

@{
    ViewData["Title"] = "Список країн";
}

<h2>Список країн</h2>

<ul>
    @foreach (var country in Model)
    {
        <li>@country</li>
    }
</ul>

<!-- Додатковий приклад з умовою -->
@if (Model.Count > 0)
{
    <p>Всього країн: @Model.Count</p>
}
else
{
    <p>Список країн порожній</p>
}